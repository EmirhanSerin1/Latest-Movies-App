{
    "hosting": {
        "public": "build/web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "headers": [{
            "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
            "headers": [{
                "key": "Access-Control-Allow-Origin",
                "value": "*"
            }]
        }],
        "trailingSlash": false
    }
}